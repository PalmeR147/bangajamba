{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
      {
          "taskName": "start",
          "args": ["start", "watch-less"]
      },
      {
          "taskName": "predeploy",
          "args": ["predeploy"]
      },
      {
          "taskName": "deploy",
          "args": ["deploy"]
      },
      {
          "taskName": "eslint",
          "args": ["eslint-report"]
      }        
  ]
}